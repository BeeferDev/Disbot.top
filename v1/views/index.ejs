												<% } %>
											<% } else { %>
												<p id="text303">Status: Unknown</p>
											<% } %>
											<% if(elementTop.serverCount > 0) { %>
												<p id="text303">Servers: <%= elementTop.serverCount %></p>
											<% } else { %>
												<p id="text303">Servers: Unknown</p>
											<% } %>
										</div>
									</div>
								</div>
							</div>
						<% }); %>
						<ul id="buttons21" class="buttons">
							<li>
								<a href="/list/certified" class="button n01">EXPLORE MORE CERTIFIED BOTS</a>
							</li>
						</ul>
					</section>
					<!-- ---------------------- -->
					<footer id="footer">
						<%- include("partials/footer.ejs") %>
					</footer>
				</div>
			</div>
		</div>
		<script src="/assets/main.js"></script>
	</body>
</html>