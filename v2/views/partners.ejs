<!DOCTYPE HTML>
<html lang="en">
	<head>
		<title>Partners | Disbot.top</title>
		<%- include("partials/head.ejs") %>
		<meta name="description" content="Expand your bots potential with disbot.top. Add your bot to our list and have it seen by heaps of people around the universe!" />
		<meta property="og:title" content="Partners | Disbot.top" />
        <link rel="stylesheet" href="/assets/main.css" />
	</head>
	<body>
		<div id="wrapper">
			<div id="main">
				<div class="inner">
					<header id="header">
                        <%- include("partials/header.ejs", { discordInfo: discordInfo }) %>
					</header>
					<!-- ---------------------- -->
					<section id="home-section">
                        <!-- <div id="container80" class="container default">
                            <div class="wrapper">
                                <div class="inner">
                                    <p id="text278">Alert Title</p>
                                    <p id="text279">Alert Message</p>
                                </div>
                            </div>
                        </div> -->
                        <p id="text44">Our Partners</p>
                        <hr id="divider59">
                        <div id="container05" class="container columns">
                            <div class="wrapper">
                                <div class="inner">
                                    <div class="inner">
                                        <% let topCounter = 0;
                                        partners.forEach(element => {
                                            let desc = element.p_desc.substring(0, 135) + '...';
                                            if(topCounter % 4 === 0) { %>
                                                </div></div></div><div id="container05" class="container columns full screen"><div class="wrapper"><div class="inner">
                                            <% } %>
                                            <div>
                                                <div id="image36" class="image">
                                                    <span class="frame"><img src="<%= element.p_image %>" alt="" /></span>
                                                </div>
                                                <p id="text42"><%= element.p_name %></p>
                                                <p id="text40"><%= desc %></p>
                                                <hr id="divider14">
                                                <ul id="buttons15" class="buttons">
                                                    <li>
                                                        <a href="<%= element.p_link %>" class="button n01"><svg><use xlink:href="/assets/icons.svg#information"></use></svg><span class="label">Learn More</span></a>
                                                    </li>
                                                </ul>
                                            </div>
                                        <%
                                            topCounter++;
                                        });
                                        %>
                                    </div>
                                </div>
                            </div>
                        </div>
					</section>
					<!-- ---------------------- -->
					<footer id="footer">

					</footer>
				</div>
			</div>
		</div>
        <script src="/assets/main.js"></script>
	</body>
</html>